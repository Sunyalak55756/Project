<?xml version="1.0" encoding="utf-8"?>
<LinearLayout

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_gravity="center">

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="517dp"

        android:layout_gravity="top"
        android:background="@mipmap/bg2"
        tools:layout_editor_absoluteY="2dp">

        <FrameLayout
            android:id="@+id/frameLayoutx"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:layout_weight="0.07"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.606"
            tools:layout_editor_absoluteX="10dp">

            <EditText
                android:id="@+id/tel"
                style="@android:style/Widget.Material.Light.AutoCompleteTextView"
                android:layout_width="250dp"
                android:layout_height="49dp"
                android:layout_gravity="right"
                android:layout_marginBottom="0dp"
                android:background="@color/zxing_custom_result_view"
                android:ems="10"
                android:hint="ex.099-9999999"
                android:inputType="date|text"
                android:textColor="@android:color/white"
                android:textColorHint="@android:color/holo_green_light" />

            <TextView
                android:id="@+id/Ic12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Tel"
                android:textAppearance="@style/TextAppearance.AppCompat"

                android:textColor="@android:color/white"
                android:textSize="18dp" />
        </FrameLayout>

        <TextView
            android:id="@+id/Log"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="8dp"
            android:layout_marginTop="28dp"

            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:layout_weight="0.28"
            android:text="แก้ไขข้อมูล"
            android:textColor="@android:color/white"
            android:textSize="28dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.017" />

        <FrameLayout
            android:id="@+id/frameLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="8dp"
            android:layout_weight="0.07"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.189"
            tools:layout_editor_absoluteX="10dp">

            <EditText
                android:id="@+id/password"
                style="@android:style/Widget.Material.Light.AutoCompleteTextView"
                android:layout_width="252dp"
                android:layout_height="49dp"
                android:layout_gravity="right"
                android:layout_marginBottom="0dp"
                android:background="@color/zxing_custom_result_view"
                android:ems="10"
                android:hint="more than 9 characters"
                android:inputType="textPassword"
                android:textColor="@android:color/white"
                android:textColorHint="@android:color/holo_green_light" />

            <TextView
                android:id="@+id/passwords"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Password"
                android:textAppearance="@style/TextAppearance.AppCompat"

                android:textColor="@android:color/white"
                android:textSize="18dp" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/frameLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:layout_weight="0.07"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.323"
            tools:layout_editor_absoluteX="10dp">

            <EditText
                android:id="@+id/name"
                style="@android:style/Widget.Material.Light.AutoCompleteTextView"
                android:layout_width="251dp"
                android:layout_height="49dp"
                android:layout_gravity="right"
                android:layout_marginBottom="0dp"
                android:background="@color/zxing_custom_result_view"
                android:ems="10"
                android:inputType="text"
                android:textColor="@android:color/white"
                android:textColorHint="@android:color/holo_green_light" />

            <TextView
                android:id="@+id/Is"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="Name"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textColor="@android:color/white"

                android:textSize="18dp" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/frameLayouta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:layout_weight="0.07"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.458"
            tools:layout_editor_absoluteX="-38dp">

            <EditText
                android:id="@+id/dob"
                style="@android:style/Widget.Material.Light.AutoCompleteTextView"
                android:layout_width="250dp"
                android:layout_height="49dp"
                android:layout_gravity="right"
                android:layout_marginBottom="0dp"
                android:background="@color/zxing_custom_result_view"
                android:ems="10"
                android:hint="ex.1995/12/01"
                android:inputType="date|text"

                android:textColor="@android:color/white"
                android:textColorHint="@android:color/holo_green_light" />

            <TextView
                android:id="@+id/I"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:text="DOB"
                android:textAppearance="@style/TextAppearance.AppCompat"

                android:textColor="@android:color/white"

                android:textSize="18dp" />
        </FrameLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/frameLayoutx0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:layout_weight="0.07"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.763"
            tools:layout_editor_absoluteX="16dp">

            <TextView
                android:id="@+id/Ic"
                android:layout_width="62dp"
                android:layout_height="30dp"
                android:layout_marginEnd="8dp"
                android:text="Image"
                android:textAppearance="@style/TextAppearance.AppCompat"

                android:textColor="@android:color/white"
                android:textSize="18dp"
                app:layout_constraintEnd_toStartOf="@+id/upload"
                app:layout_constraintHorizontal_bias="0.052"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/upload"
                android:layout_width="101dp"
                android:layout_height="36dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="16dp"
                android:text="Browse..."
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.374"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/submit"
        android:layout_width="422dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-2dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:background="@mipmap/blue"
        android:text="Submit"
        android:textStyle="bold"
        android:textAllCaps="false"
        android:textColor="@android:color/white"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.933" />
</LinearLayout>